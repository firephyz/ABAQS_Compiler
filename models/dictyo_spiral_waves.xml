<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2">
  <model id="dictyostelium_qs" areaUnits="area" timeUnits="second">
    <listOfFunctionDefinitions>
      <!-- This specific function is the I(S), the preprocessed response to cAMP. -->
      <!-- As the paper on spiral oscillations does, this can easily be replaced -->
      <!-- by different preprocessing functions. -->
      <functionDefinition id="preprocess">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <lambda>
            <bvar>
              <ci> c </ci>
            </bvar>
            <apply>
              <times/>
              <cn> 1.0 </cn>
              <apply>
                <log/>
                <apply>
                  <plus/>
                  <cn> 1.0 </cn>
                  <apply>
                    <divide/>
                    <ci> c </ci>
                    <cn> 1.0 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="heaviside">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <lambda>
            <bvar>
              <ci> x </ci>
            </bvar>
            <piecewise>
              <piece>
                <cn> 1.0 </cn>
                <apply>
                  <gt/>
                  <ci> x </ci>
                  <cn> 0 </cn>
                </apply>
              </piece>
              <otherwise>
                <cn> 0 </cn>
              </otherwise>
            </piecewise>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="stoich">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <lambda>
            <!--		Must define stoicastic noise function	    -->
          </lambda>
        </math>
      </functionDefinition>
    </listOfFunctionDefinitions>
    <listOfCompartments>
      <compartment id="SIM_SPACE"		   constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="AUTO_cAMP" compartment="SIM_SPACE" hasOnlySubstanceUnits="false" boundaryCondition="false"	constant="false"/>
      <species id="CELL_dict" compartment="SIM_SPACE" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="CELL_dict_activator" constant="false"/>
      <parameter id="CELL_dict_repressor" constant="false"/>
      <!-- Needs valid values for the FHN model -->
      <parameter id="fhn_epsilon" value="0.0" constant="true"/>
      <parameter id="fhn_gamma" value="0.0" constant="true"/>
      <parameter id="fhn_c" value="0.0" constant="true"/>
      <!-- These parameters respectively correspond to alpha_f, J, alpha_0 and D -->
      <parameter id="AUTO_cAMP_inflow" value="0.0" constant="true"/>
      <parameter id="AUTO_cAMP_outflow" value="0.0" constant="true"/>
      <parameter id="CELL_dict_base_amount" value="0.0" constant="true"/>
      <parameter id="CELL_dict_output_amount" value="0.0" constant="true"/>
    </listOfParameters>
    <listOfInitialAssignments>
      <initialAssignment symbol="activator">
        <math xmlns="http://www.w3.org/1998/Math/MathML"          xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <cn> 0 </cn>
        </math>
      </initialAssignment>
      <initialAssignment symbol="repressor">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <cn> 0 </cn>
        </math>
      </initialAssignment>
      <initialAssignment symbol="cAMP">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <cn> 0 </cn>
        </math>
      </initialAssignment>
    </listOfInitialAssignments>
    <listOfRules>
      <rateRule variable="CELL_dict_activator">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <plus/>
            <ci> activator </ci>
            <apply>
              <times/>
              <cn> -0.333333 </cn>
              <apply>
                <power/>
                <ci> activator </ci>
                <cn> 3 </cn>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> -1.0 </cn>
              <ci> repressor </ci>
            </apply>
            <apply>
              <ci> preprocessor </ci>
              <ci> cAMP </ci>
            </apply>
            <apply>
              <ci> stoich </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule variable="CELL_dict_repressor">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <times/>
            <ci> fhn_epsilon </ci>
            <apply>
              <plus/>
              <ci> activator>
              </ci>
              <apply>
                <times/>
                <cn> -1.0 </cn>
                <ci> fhn_gamma </ci>
                <ci> repressor </ci>
              </apply>
              <ci> fhn_c </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule variable="AUTO_cAMP">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <plus/>
            <ci> AUTO_cAMP_inflow </ci>
            <ci> CELL_dict_base_amount </ci>
            <apply>
              <times/>
              <ci> CELL_dict_output_amount </ci>
              <apply>
                <ci> heaviside </ci>
                <ci> CELL_activator </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> -1 </cn>
              <ci> AUTO_cAMP_outflow </ci>
              <ci> AUTO_cAMP </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
