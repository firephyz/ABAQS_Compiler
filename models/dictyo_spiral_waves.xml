<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2"  xmlns:spatial="http://www.sbml.org/sbml/level3/version1/spatial/version1" spatial:required="true">
  <model id="dictyostelium qs" areaUnits="area" timeUnits="second">
    <listOfFunctionDefinitions>
      <!-- This specific function is the I(S), the preprocessed response to cAMP. -->
      <!-- As the paper on spiral oscillations does, this can easily be replaced -->
      <!-- by different preprocessing functions. -->
      <functionDefinition id="preprocess">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <lambda>
            <bvar>
              <ci> c </ci>
            </bvar>
            <apply>
              <times/>
              <cn> 1.0 </cn>
              <apply>
                <log/>
                <apply>
                  <add/>
                  <cn> 1.0 </cn>
                  <apply>
                    <div/>
                    <ci> c </ci>
                    <cn> 1.0 </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="heaviside">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <lambda>
            <bvar>
              <ci> x </ci>
            </bvar>
            <piecewise>
              <piece>
                <cn> 1.0 </cn>
                <apply>
                  <gt/>
                  <ci> x </ci>
                  <cn> 0 </cn>
                </apply>
              </piece>
              <otherwise>
                <cn> 0 </cn>
              </otherwise>
            </piecewise>
          </lambda>
        </math>
      </functionDefinition>
      <functionDefinition id="stoich">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <lambda>
            <!--		Must define stoicastic noise function	    -->
          </lambda>
        </math>
      </functionDefinition>
    </listOfFunctionDefinitions>
    <listOfCompartments>
      <compartment id="extracellular"		   spatialDimensions="2" size="1" constant="true"/>
    </listOfCompartments>
    <listOfjuk>
      <species id="cAMP"        compartment="extracellular"        boundaryCondition="false"	              constant="false"/>
    </listOfSpecies>
    <!-- Various parameters and variables used in modeling the cAMP response -->
    <listOfParameters>
      <parameter id="activator" constant="false"/>
      <parameter id="repressor" constant="false"/>
      <!-- Needs valid values for the FHN model -->
      <parameter id="fhn_epsilon" value="0.0" constant="true"/>
      <parameter id="fhn_gamma" value="0.0" constant="true"/>
      <parameter id="fhn_c" value="0.0" constant="true"/>
      <!-- These parameters respectively correspond to alpha_f, J, alpha_0 and D -->
      <parameter id="cAMP_inflow" value="0.0" constant="true"/>
      <parameter id="degrate_rate" value="0.0" constant="true"/>
      <parameter id="leakage_rate" value="0.0" constant="true"/>
      <parameter id="spike_amount" value="0.0" constant="true"/>
    </listOfParameters>
    <listOfInitialAssignments>
      <initialAssignment symbol="activator">
        <math xmlns="http://www.w3.org/1998/Math/MathML"          xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <cn> 0 </cn>
        </math>
      </initialAssignment>
      <initialAssignment symbol="repressor">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <cn> 0 </cn>
        </math>
      </initialAssignment>
      <initialAssignment symbol="cAMP">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <cn> 0 </cn>
        </math>
      </initialAssignment>
    </listOfInitialAssignments>
    <listOfRules>
      <rateRule variable="activator">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <plus/>
            <ci> activator </ci>
            <apply>
              <times/>
              <cn> -0.333333 </cn>
              <apply>
                <power/>
                <ci> activator </ci>
                <cn> 3 </cn>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> -1.0 </cn>
              <ci> repressor </ci>
            </apply>
            <apply>
              <preprocessor/>
              <ci> cAMP </ci>
            </apply>
            <apply>
              <stoich/>
            </apply>
          </apply>
        </math>
      </rateRule>
      <rateRule variable="repressor">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <times/>
            <ci> fhn_epsilon </ci>
            <apply>
              <plus/>
              <ci> activator>
              </ci>
              <apply>
                <times/>
                <cn> -1.0 </cn>
                <ci> fhn_gamma </ci>
                <ci> repressor </ci>
              </apply>
              <ci> fhn_c </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
      <!-- Needs further work to specify spatial rules and model multiple cells -->
      <rateRule variable="cAMP">
        <math xmlns="http://www.w3.org/1998/Math/MathML"	      xmlns:sbml="http://www.sbml.org/sbml/level3/version2/core">
          <apply>
            <plus/>
            <ci> cAMP_inflow </ci>
            <ci> leakage_rate </ci>
            <apply>
              <times/>
              <ci> spike_amount </ci>
              <apply>
                <heaviside/>
                <ci> activator </ci>
              </apply>
            </apply>
            <apply>
              <times/>
              <cn> -1 </cn>
              <ci> degrate_rate </ci>
              <ci> cAMP </ci>
            </apply>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
  </model>
</sbml>
